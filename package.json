  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aseprano/buraco_tables/issues"
  },
  "homepage": "https://github.com/aseprano/buraco_tables#issues",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "jasmine-ts",
    "test:live": "nodemon --watch 'tests' --ext 'ts' --exec 'jasmine-ts'",
    "coverage": "nyc npm test report --reporter=text-lcov | coveralls",
    "start": "ts-node src/index.ts",
    "start:live": "nodemon --watch 'src' --ext 'ts' --exec 'ts-node src/index.ts'",
    "startjs": "node build/index.js",
    "prepare": "npm run build",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aseprano/buraco_tables.git"
  },
  "dependencies": {
    "@darkbyte/herr": "^0.32",
    "body-parser": "^1.19.0",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^3.6.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.31",
    "@types/uuid": "^8.3.0",
    "coveralls": "^3.1.0",
    "jasmine": "3.7",
    "jasmine-ts": "^0.3.3",
    "jasminewd2": "^2.2.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
